{
  "expo": {
    "name": "KinikaHub",
    "slug": "KinikaHub",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/klinikahub.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/klinikahub.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kinikahub.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.kinikahub.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/klinikahub-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": ["RECEIVE_BOOT_COMPLETED", "WAKE_LOCK", "VIBRATE"],
      "useNextNotificationsApi": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "217cf780-89ca-4c63-92cb-961b76ec7b2a"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/217cf780-89ca-4c63-92cb-961b76ec7b2a"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "newArchEnabled": true
          },
          "ios": {
            "newArchEnabled": true
          }
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/klinikahub-icon.png",
          "color": "#0891b2",
          "defaultChannel": "reminders",
          "sounds": ["./assets/reminder2.mp3"]
        }
      ]
    ]
  }
}
